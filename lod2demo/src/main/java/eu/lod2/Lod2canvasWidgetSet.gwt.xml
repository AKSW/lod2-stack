<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC
    "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN"
    "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module>
<!--
     This file declares the addons which the application uses and depends on.
     Also these references point to addons which require possibly a GWT compilation phase.
     The binding between Vaadin JAVA en the gwt libs is done in the classes found in the directory
     client/ui/
-->
    <inherits name="com.vaadin.terminal.gwt.DefaultWidgetSet" />
    <inherits name='org.vaadin.gwtgraphics.GWTGraphics'/>

    <inherits name="org.vaadin.codelabel.CodelabelWidgetset" />

    <!-- compile GWT code only for Firefox browser => speeds compilation -->
		<set-property name="user.agent" value="gecko1_8"/> 

    <inherits name="com.vaadin.ui.addon.usemapimage.UsemapimageWidgetset" />
</module>
