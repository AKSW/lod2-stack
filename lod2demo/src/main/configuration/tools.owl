    <lod2:Tool rdf:about="http://localhost/ontowiki">
      <lod2:responsible rdf:resource="&lod2;SebastianTramp"/>
      <lod2:contributed_by_partner rdf:resource="&lod2eu;ulei"/>
      <lod2:license rdf:resource="&lod2;Apache_2.0"/>
      <rdfs:label xml:lang="en">OntoWiki</rdfs:label>
      <lod2:tool_homepage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://aksw.org/Projects/OntoWiki/</lod2:tool_homepage>
      <lod2:tool_description xml:lang="en">&lt;b&gt;OntoWiki&lt;/b&gt; facilitates the visual presentation of a knowledge base as an information map, with different views on instance data. It enables intuitive authoring of semantic content, with an inline editing mode for editing RDF content, similar to WYSIWIG for text documents.</lod2:tool_description>
    </lod2:Tool>

